<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/authFragment"
        android:name="com.example.moviedb.ui.auth.AuthFragment"
        android:label="AuthFragment"
        tools:layout="@layout/fg_auth">
        <action
            android:id="@+id/action_authFragment_to_movieFragment2"
            app:destination="@id/movieFragment2" />
    </fragment>
    <fragment
        android:id="@+id/movieFragment2"
        android:name="com.example.moviedb.ui.movie.MovieFragment"
        android:label="MovieFragment"
        tools:layout="@layout/fg_movies"/>
</navigation>